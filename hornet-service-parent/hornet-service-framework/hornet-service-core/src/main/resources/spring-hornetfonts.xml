<?xml version="1.0" encoding="UTF-8"?>
<beans
    xmlns="http://www.springframework.org/schema/beans"
    xmlns:context="http://www.springframework.org/schema/context"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd">
        
	<bean id="LiberationSansFamily"
	class="net.sf.jasperreports.engine.fonts.SimpleFontFamily">
	<property name="name" value="Liberation Sans"/>
	<property name="normal" value="hornet/framework/font/LiberationSans-Regular.ttf"/>
	<property name="bold" value="hornet/framework/font/LiberationSans-Bold.ttf"/>
	<property name="italic" value="hornet/framework/font/LiberationSans-Italic.ttf"/>
	<property name="boldItalic" value="hornet/framework/font/LiberationSans-BoldItalic.ttf"/>
	<property name="pdfEncoding" value="Identity-H"/>
	<property name="pdfEmbedded" value="true"/>
	</bean>

</beans>